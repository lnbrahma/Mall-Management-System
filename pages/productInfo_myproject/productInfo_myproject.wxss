/* pages/productInfo_myproject/productInfo_myproject.wxss */
.container {
  background-color: white;
}
.addproject {
  position: relative;
  width: 650rpx;
  height: 300rpx;
  border: 2rpx solid grey;
  border-radius: 30rpx;
  margin: 50rpx auto;
  background-color: white;
}
.addproject image {
  position: absolute;
  top: 50%;
  left:50%;
  width: 60rpx;
  height: 60rpx;
  margin-left: -30rpx;
  margin-top: -30rpx;
}
.details {
  width: 650rpx;
  height: 300rpx;
  border-radius: 30rpx;
  margin: 50rpx auto;
  padding: 30rpx;
  background-color: rgb(236, 236, 236);
}
.title {
  font-size: 40rpx;
  line-height: 80rpx;
}
.date {
  line-height: 60rpx;
}